{
  "expo": {
    "name": "Gemify",
    "slug": "gemify-universal",
    "jsEngine": "hermes",
    "scheme": "gemify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/your-project-id"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartlemon.gemify",
      "associatedDomains": [
        "applinks:staging.gemify.app"
      ],
      "buildNumber": "48",
      "usesAppleSignIn": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "audio"
        ]
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.acme.myapp",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.premission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "organization": "smartlemon",
          "project": "gemify-mobile",
          "url": "https://sentry.io/",
          "note": "process.env.SENTRY_AUTH_TOKEN"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-apple-authentication",
      "expo-router",
      "expo-build-properties",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8da37ade-cec9-4b62-8e7b-5f61c31b043e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "loschenbd"
  }
}
